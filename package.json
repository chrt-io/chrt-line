{
    "name": "chrt-line",
    "version": "0.0.44",
    "description": "Line chart component for Chrt",
    "keywords": [
        "chrt",
        "charts",
        "linecharts",
        "areacharts",
        "visualization"
    ],
    "type": "module",
    "main": "src/index.js",
    "module": "src/index.js",
    "jsdelivr": "dist/chrt-line.min.js",
    "unpkg": "dist/chrt-line.min.js",
    "exports": {
        "umd": "./dist/chrt-line.min.js",
        "default": "./dist/chrt-line.esm.js"
    },
    "watch": {
        "build": {
            "patterns": [
                "src"
            ],
            "extensions": "js,jsx"
        }
    },
    "scripts": {
        "test": "TZ=UTC jest",
        "postversion": "rollup -c",
        "build": "rollup -c",
        "lint": "eslint src --fix",
        "watch": "npm-watch",
        "dev": "budo test/chrts/index.js:bundle.js --live --dir test/chrts  -- -t [ babelify --extensions .js  --global true ]",
        "prepare": "husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint",
            "pre-push": "npm run lint"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@gitlab.com/chrt/chrt-line.git"
    },
    "author": "chrt",
    "license": "MIT",
    "bugs": {
        "url": "https://gitlab.com/chrt/chrt-line/issues"
    },
    "homepage": "chrt.io",
    "dependencies": {
        "chrt-interpolations": "0.0.14",
        "chrt-object": "0.0.16"
    },
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@babel/plugin-transform-modules-commonjs": "^7.21.2",
        "@babel/plugin-transform-runtime": "^7.21.4",
        "@babel/preset-env": "^7.21.4",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@wwa/rollup-plugin-terser": "^1.0.1",
        "babel-jest": "^29.5.0",
        "babel-plugin-root-import": "^6.6.0",
        "babelify": "^10.0.0",
        "budo": "^11.8.4",
        "chrt": "0.0.154",
        "eslint": "^8.39.0",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "npm-watch": "^0.11.0",
        "rollup": "3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "watch": "^1.0.2"
    },
    "files": [
        "dist/**/*.js",
        "src/**/*.js"
    ]
}
